<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="PlantBase">
    <defName>SW_Plant_ThornyBush</defName>
    <label>thorny bush</label>
    <description>A very thorny bush. It greatly slows down anyone who moves over it.</description>
    <statBases>
      <MaxHitPoints>85</MaxHitPoints>
      <Beauty>-1</Beauty>
      <BeautyOutdoors>1</BeautyOutdoors>
      <Nutrition>0.05</Nutrition>
    </statBases>
    <graphicData>
      <texPath>Things/Plant/ThornyBush</texPath>
      <graphicClass>Graphic_Random</graphicClass>
    </graphicData>
    <altitudeLayer>Item</altitudeLayer>
    <selectable>true</selectable>
	<pathCost>50</pathCost>
    <ingestible>
      <canAutoSelectAsFoodForCaravan>false</canAutoSelectAsFoodForCaravan>
    </ingestible>
    <plant>
      <dieIfLeafless>false</dieIfLeafless>
	  <fertilitySensitivity>0.6</fertilitySensitivity>
      <harvestTag>Standard</harvestTag>
      <sowWork>750</sowWork>
      <sowTags>
        <li>Ground</li>
      </sowTags>
      <topWindExposure>0.1</topWindExposure>
      <growDays>3.5</growDays>
      <visualSizeRange>0.3~1.05</visualSizeRange>
      <wildOrder>1</wildOrder>
	  <pollution>Any</pollution>
    </plant>
  </ThingDef>

</Defs>
